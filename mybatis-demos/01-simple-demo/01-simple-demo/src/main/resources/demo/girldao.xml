<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuki.dao.GirlDao">

    <!-- queryGirlList-->
    <select id="queryGirlList" resultType="com.yuki.entity.Girl">
        select
            id,
            name,
            sex,
            borndate as birth,
            phone,
            photo,
            boyfriend_id
        from beauty
    </select>
</mapper>